{
  "name": "@kui-shell/core",
  "version": "2.32.0",
  "description": "An Electron-based shell for cloud-native development",
  "homepage": "https://github.com/IBM/kui#readme",
  "license": "Apache-2.0",
  "author": "Nick Mitchell",
  "bugs": {
    "url": "https://github.com/IBM/kui/issues/new"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IBM/kui.git"
  },
  "keywords": [
    "OpenWhisk",
    "CLI",
    "UI",
    "kubernetes",
    "visualization",
    "serverless"
  ],
  "contributors": [
    "Mengting Yan"
  ],
  "main": "main/main.js",
  "bin": {
    "kui": "bin/kui",
    "kubectl-kui": "bin/kubectl-kui"
  },
  "scripts": {
    "prepack": "rm -rf dist && mkdir dist && tar -C ../../build/packages/$(basename `pwd`)/src --exclude test --exclude node_modules -cf - . | tar -C dist -xf - && find dist -type l -exec rm {} \\; && cp -a dist/* .",
    "postpack": "for i in dist/*; do rm -rf ./`basename $i`; done"
  },
  "dependencies": {
    "app-module-path": "2.2.0",
    "colors": "1.3.3",
    "columnify": "1.5.4",
    "debug": "4.1.1",
    "electron-context-menu": "0.12.1",
    "electron-squirrel-startup": "1.0.0",
    "expand-home-dir": "0.0.3",
    "extract-zip": "1.6.7",
    "fs-extra": "7.0.1",
    "js-beautify": "1.10.0",
    "json-colorizer": "2.2.0",
    "mkdirp": "0.5.1",
    "needle": "2.3.1",
    "open": "6.2.0",
    "pretty-ms": "5.0.0",
    "properties-parser": "0.3.1",
    "tmp": "0.1.0",
    "yargs-parser": "13.1.0"
  },
  "devDependencies": {
    "request-promise": "4.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "89de9f78e8a1a2bdd29d2e17d7c608ab006c6d32"
}
